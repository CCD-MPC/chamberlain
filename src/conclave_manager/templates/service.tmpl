{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata":
    {
        "name": "{{{SERVICE_NAME}}}"
    },
    "spec":
    {
        "selector":
        {
            "app": "{{{APP_NAME}}}"
        },
        "type": "ClusterIP",
        "sessionAffinity": "None",
        "ports":
        [
            {
                "name": "{{{PORT}}}-tcp",
                "protocol": "TCP",
                "port": {{{PORT}}},
                "targetPort": {{{PORT}}}
            }
        ]
    }
}