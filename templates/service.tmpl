{
    "apiVersion": "v1",
    "kind": "Template",
    "metadata":
    {
        "name": "conclave-test-template"
    },
    "objects":
    [
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata":
            {
                "name": "{{{SERVICE_NAME}}}"
            },
            "spec":
            {
                "selector":
                {
                    "app": "{{{SERVICE_NAME}}}"
                },
                "type": "ClusterIP",
                "sessionAffinity": "None",
                "ports":
                [
                    {
                        "protocol": "TCP",
                        "port": 5000
                    }
                ]
            }
        }
    ]
}